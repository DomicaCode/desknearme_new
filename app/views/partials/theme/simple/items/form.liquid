{% liquid
  if item.id
    assign path = 'update'
  else
    assign path = 'create'
  endif
  assign uuid = '' | uuid
  assign item_uuid = item.uuid | default: uuid
  assign image_delete_token = authenticity_token | default: context.authenticity_token
%}

<div class="container flex flex-wrap items-center mx-auto">
  <input type="hidden" name="image_delete_token" id="image_delete_token" value="{{ image_delete_token }}" />
  <form action="/dashboard/items/{{ path }}" method="post" id="property_frm">
    {% include 'authenticity_token' %}

    {% if item.id %}
    <input type="hidden" name="item[id]" value="{{ item.id }}" />
    {% endif %}
    <input type="hidden" name="item[uuid]" value="{{ item_uuid }}" />

    <div class="my-3">
      <label class="block" for="name">{{ 'app.items.form.name' | t }}</label>
      <input class="w-2/3 form-input" type="text" id="name" name="item[name]" value="{{ item.name }}" autofocus required />
      {% include 'theme/simple/field_error', errors: object.errors.name %}
    </div>

    <div class="mt-3">
      <label class="block">{{ 'app.items.form.photos' | t }}</label>
      {% include 'theme/simple/photos/uppy_area' %}
    </div>

    {% if path == 'update' %}
    <div class="mt-3" style="display:block;">
    {% else %}
    <div class="mt-3" style="display:none;">
    {% endif %}
      <label class="block" for="price">Images</label>
      <a id="myBtnViewPhotos" style="cursor:pointer;" herf="javascript:void();">View</a>
    </div>

    <!-- The Modal -->
    <div id="myModalViewPhotos" class="modal">

      <!-- Modal content -->
      <div class="modal-content">
        <span id="remove_photo_msg" style="dislay:none; color:green;"></span>
        <span class="close">&times;</span>
        <table name="price_table" id="price_table" border="1" cellpadding="1" cellspacing="0">
        <tr>
          <th align="left" style="width:280px;">Image</th>
          <th align="left">Action</th>
        </tr>

        {% if item.photos.size > 0 %}

        {% for p in item.photos limit: 5 %}
        <tr id="remove_image_tr_{{ p.id }}"">
          <td><img width="200" height="100" src="{{ p.photo.versions.uncropped }}" alt="{{ item.name }}"></td>
          <td><a class="remove_image_a" id="remove_image_{{ p.id }}" href="javascript:void(0);">Remove</a></td>
        </tr>
        {% endfor %}
        
        {% endif %}        
        </table>
      </div>

    </div>

    
    <div class="mt-3">
      <label class="block" for="category">{{ 'app.items.form.category' | t }}</label>
      <select class="form-select" id="category" name="item[category]">
        {% for category in lib.categories %}
          {% assign ident = category | split: '/' | size | minus: 1 | times: 2 %}
          <option value="{{ category }}" {% if item.category == category %}selected{% endif %}>
              {% for step in (1..ident) %} {% print "&nbsp;" %} {% endfor %}
              {{ 'categories.' | append: category | t }}
          </option>
        {% endfor %}
      </select>

      {% include 'theme/simple/field_error', errors: object.errors.category %}
    </div>

    <div class="mt-3">
      <label class="block" for="short_description">Short Description</label>
      <textarea class="w-full form-input" type="text" id="short_description" name="item[short_description]" rows="3" required>{{ item.short_description }}</textarea>

      {% include 'theme/simple/field_error', errors: item.errors.short_description %}
    </div>


    <div class="mt-3">
      <label class="block" for="description">{{ 'app.items.form.description' | t }}</label>
      <textarea class="w-full form-input" type="text" id="description" name="item[description]" rows="3" required>{{ item.description }}</textarea>

      {% include 'theme/simple/field_error', errors: object.errors.description %}
    </div>

    <div class="mt-3">
      <label class="block" for="location_name">Location Name</label>
      <input class="w-2/3 form-input" type="text" id="location_name" name="item[location_name]" value="{{ item.location_name }}" autofocus required />

      {% include 'theme/simple/field_error', errors: item.errors.location_name %}
    </div>

    <div class="mt-3">
      <label class="block" for="location_address">Location Address</label>
      <textarea class="w-full form-input" type="text" id="location_address" name="item[location_address]" rows="3" required>{{ item.location_address }}</textarea>

      {% include 'theme/simple/field_error', errors: item.errors.location_address %}
    </div>

    <div class="mt-3">
      <label class="block" for="zip_code">Zip Code</label>
      <input class="w-2/3 form-input" type="text" id="zip_code" name="item[zip_code]" value="{{ item.zip_code }}" autofocus required />

      <span style="color:red; display:none;" id="err_zip_code"></span>
      {% include 'theme/simple/field_error', errors: item.errors.zip_code %}
    </div>

    <div class="mt-3">
      <label class="block" for="location_type">Location Type</label>
      <select class="form-select" id="location_type" name="item[location_type]">
        <option value="Business" {% if item.location_type == 'Business' %}selected{% endif %}>Business</option>
        <option value="Co-Working" {% if item.location_type == 'Co-Working' %}selected{% endif %}>Co-Working</option>
        <option value="Public" {% if item.location_type == 'Public' %}selected{% endif %}>Public</option>
        <option value="Executive Suites" {% if item.location_type == 'Executive Suites' %}selected{% endif %}>Executive Suites</option>
      </select>

      {% include 'theme/simple/field_error', errors: item.errors.location_type %}
    </div>

    <div class="mt-3" style="display:none;">
      <label class="block" for="pricing_type">Pricing Type</label>
      <select class="form-select" id="pricing_type" name="item[pricing_type]">
        <option value="Per Hour" {% if item.pricing_type == 'Per Hour' %}selected{% endif %}>Per Hour</option>
        <option value="Per Day" {% if item.pricing_type == 'Per Day' %}selected{% endif %}>Per Day</option>
        <option value="Per work week (5 days)" {% if item.pricing_type == 'Per work week (5 days)' %}selected{% endif %}>Per work week (5 days)</option>
        <option value="Per calendar week (7 days)" {% if item.pricing_type == 'Per calendar week (7 days)' %}selected{% endif %}>Per calendar week (7 days)</option>
        <option value="Per calendar month" {% if item.pricing_type == 'Per calendar month' %}selected{% endif %}>Per calendar month</option>
      </select>

      {% include 'theme/simple/field_error', errors: item.errors.pricing_type %}
    </div>

     <div class="mt-3" style="display:none;">
      <label class="block" for="price">{{ 'app.items.form.price' | t }}</label>
      <!-- TODO: Add currency prefix $[input]  -->
      <input class="w-32 form-input" type="number" id="price" name="item[price]"
        value="{{ item.price | default: 1000 | pricify_cents | replace_regex: '[\$\,]', '' }}" />
      {% include 'theme/simple/field_error', errors: object.errors.price %}
    </div>

    <div class="mt-3">
      <label class="block" for="price">{{ 'app.items.form.price' | t }}</label>
      <a id="myBtn" style="cursor:pointer;" herf="javascript:void();">Add/View Price</a>
    </div>

    <!-- Trigger/Open The Modal -->
    <!--<button id="myBtn">Open Modal</button>-->

    <!-- The Modal -->
    <div id="myModal" class="modal">

      <!-- Modal content -->
      <div class="modal-content">
        <span class="close">&times;</span>
        <table name="price_table" id="price_table" border="1" cellpadding="1" cellspacing="0">
        <tr>
          <th align="left" style="width:280px;">Pricing Type:</th>
          <th align="left">Price</th>
        </tr>
        <tr>
          <td>Per Hour</td>
          <td><input type="text" name="item[price_per_hour]" id="price_per_hour" value="{{ item.price_per_hour }}"></td>
        </tr>
        <tr>
          <td>Per Day</td>
          <td><input type="text" name="item[price_per_day]" id="price_per_day" value="{{ item.price_per_day }}"></td>
        </tr>
        <tr>
          <td>Per Work Week(5 Days)</td>
          <td><input type="text" name="item[price_per_work_week]" id="price_per_work_week" value="{{ item.price_per_work_week }}"></td>
        </tr>
        <tr>
          <td>Per Calender Week(7 Days)</td>
          <td><input type="text" name="item[price_per_calender_week]" id="price_per_calender_week" value="{{ item.price_per_calender_week }}"></td>
        </tr>
        <tr>
          <td>Per Month</td>
          <td><input type="text" name="item[price_per_month]" id="price_per_month" value="{{ item.price_per_month }}"></td>
        </tr>
        
        </table>
      </div>

    </div>


    <div class="mt-3">
        <label class="block" for="quantity">Quantity</label>
        <input class="w-32 form-input" type="number" id="quantity" name="item[quantity]" value="{{ item.quantity | default: 1 }}" />
        {% include 'theme/simple/field_error', errors: item.errors.quantity %}
    </div>


    <div class="mt-3">
      <label class="block" for="availability">Availability</label>
      <select class="form-select" id="availability" name="item[availability]">
        <option value="Mon to Fri (9:00AM - 6:00PM)" {% if item.availability == 'Mon to Fri (9:00AM - 6:00PM)' %}selected{% endif %}>Mon to Fri (9:00AM - 6:00PM)</option>
        <option value="Customize" {% if item.availability == 'Customize' %}selected{% endif %}>Customize</option>
      </select>

      {% include 'theme/simple/field_error', errors: item.errors.availability %}
    </div>

     {% if item.availability == 'Customize' %}
